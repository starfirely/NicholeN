jQuery(document).ready(function($){
	// Toggle nav menu on button click
	var $root = $('html, body'),
		menu_toggle = $('.menu-toggle');
	
	menu_toggle.pn('click', function()){
		var $this = $(this);
	
		$this.toggleClass('close');
		$('.menu, .menu-nav, menu-nav-itme').toggleClass('show');
	});
});